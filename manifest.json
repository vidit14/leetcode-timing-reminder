{
  "manifest_version": 3,
  "name": "LeetCode Timer Prompt",
  "version": "1.0",
  "description": "Automatically prompt to start timer when opening LeetCode problems",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}